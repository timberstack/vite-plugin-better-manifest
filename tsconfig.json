{
    "compilerOptions": {
      "target": "ES2024",
      "module": "NodeNext", 
      "moduleResolution": "nodenext",
      "allowImportingTsExtensions": true,
      "noEmit": true,
      "esModuleInterop": true,                        
      "forceConsistentCasingInFileNames": true, 
      "strict": true, 
      "skipLibCheck": true
    }
